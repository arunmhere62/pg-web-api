generator client {
  provider      = "prisma-client-js"
  output        = "../node_modules/@prisma/client-management"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_MANAGEMENT_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
  @@index([plan_id], map: "fk_subscription_payment_plan")
  @@index([subscription_id], map: "fk_subscription_payment_subscription")
  @@index([created_at], map: "idx_created_at")
  @@index([order_id], map: "idx_order_id")
  @@index([organization_id], map: "idx_organization_id")
  @@index([status], map: "idx_status")
  @@index([user_id], map: "idx_user_id")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model subscription_plans {
  s_no                   Int                     @id @default(autoincrement())
  name                   String                  @db.VarChar(100)
  description            String?                 @db.Text
  duration               Int
  price                  Decimal                 @db.Decimal(10, 2)
  currency               String                  @default("INR") @db.VarChar(10)
  features               Json?
  max_pg_locations       Int?
  max_tenants            Int?
  is_active              Boolean                 @default(true)
  created_at             DateTime                @default(now()) @db.Timestamp(0)
  updated_at             DateTime                @default(now()) @db.Timestamp(0)
  max_rooms              Int?
  max_beds               Int?
  max_employees          Int?
  max_users              Int?
  max_invoices_per_month Int?
  max_sms_per_month      Int?

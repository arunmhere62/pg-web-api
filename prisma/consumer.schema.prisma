  tracking_id        String?                            @db.VarChar(255)
  bank_ref_no        String?                            @db.VarChar(255)
  payment_mode       String?                            @db.VarChar(100)
  status_code        String?                            @db.VarChar(50)
  status_message     String?                            @db.Text
  failure_message    String?                            @db.Text
  response_data      Json?
  metadata           Json?
  created_at         DateTime                           @default(now()) @db.Timestamp(0)
